{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@azure-utils/feature-flags",
  "version": "0.1.5",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts",
    "./allocate": "./src/allocate.ts",
    "./client": "./src/client.ts",
    "./rollout": "./src/rollout.ts",
    "./service": "./src/service.ts",
    "./validate": "./src/validate.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "README.md"
    ],
    "exclude": [
      ".github",
      ".yarn",
      "app",
      "scripts",
      "tests",
      "esm",
      "cjs",
      ".yarnrc.yml",
      "*.js",
      "*.d.ts",
      "*.html",
      "vite.config.ts",
      "yarn.lock",
      "*.json"
    ]
  }
}